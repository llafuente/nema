{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && ava --verbose ./dist/test/all.test.js --fail-fast",
    "testcv": "npm run build && nyc ava --verbose ./dist/test/all.test.js && nyc report --reporter html --report-dir coverage/html",
    "pretty": "prettier --write --parser typescript --print-width 120 \"src/**/*.ts\"",
    "lint": "npm run pretty && tslint --force --fix -c ./tslint.json --type-check -p ./tsconfig.json",
    "build": "tsc -p ./tsconfig.json",
    "start": "npm run build && node ./dist/src/index.js",
    "nodemon": "npm start || exit 1"
  },
  "author": "llafuente@noboxout.com",
  "license": "MIT",
  "dependencies": {
    "async": "2.6.0",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "jsontoxml": "0.1.0",
    "jsonwebtoken": "8.2.0",
    "lodash": "4.17.5",
    "morgan": "1.9.0",
    "multer": "1.3.0",
    "swagger-ui-express": "latest",
    "typescript": "2.7.2"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/node": "9.4.6",
    "@types/sinon": "4.3.0",
    "@types/supertest": "2.0.4",
    "ava": "0.25.0",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "sinon": "4.4.5",
    "supertest": "3.0.0",
    "tslint": "5.9.1"
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  }
}
